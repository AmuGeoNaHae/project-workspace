    // 아이템갯수 조절 삭제 햠수
    function selectItemClose(number) {
      $(`#${number}`).remove();
      setTotalPrice();
      document.querySelector(`.btn_number_${number}`).classList.remove('active');

      if ($('.select_item').length === 0) {
        $('.choice_select').css('display', 'none');
